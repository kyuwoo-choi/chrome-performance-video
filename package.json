{
  "name": "chrome-performance-video",
  "version": "1.0.0-beta.2",
  "main": "dist/chrome-performance-video.js",
  "license": "MIT",
  "description": "create and play performance video out of chrome devtools performance profile",
  "repository": "https://github.com/kyuwoo-choi/chrome-performance-video.git",
  "author": "kyuwoo-choi",
  "scripts": {
    "example": "parcel example/index.html --out-dir .cache/example",
    "build": "parcel build lib/index.js --out-file chrome-performance-video.js --global chromePerformanceVideo",
    "docs": "jsdoc2md ./lib/**/*.js > docs/api.md ",
    "test": "cypress run",
    "test:watch": "cypress open"
  },
  "devDependencies": {
    "@babel/core": "7.6.4",
    "@babel/plugin-transform-runtime": "7.6.2",
    "@babel/preset-env": "7.6.3",
    "cypress": "3.5.0",
    "parcel-bundler": "1.12.4",
    "prettier": "1.18.2"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-transform-runtime"
    ]
  },
  "keywords": [
    "chrome",
    "devtools",
    "performance",
    "timeline",
    "video",
    "movie",
    "webm",
    "canvas"
  ]
}
